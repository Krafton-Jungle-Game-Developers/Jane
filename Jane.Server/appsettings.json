{
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001",
        "ClientCertificateMode": "AllowCertificate",
        "Protocols": "Http1AndHttp2",
        "SslProtocols": [ "Tls12" ],
        "Certificate": {
          "Path": "./server.crt",
          "KeyPath": "./server.key",
          "AllowInvalid": true
        }
      }
    },
    "AllowedHosts": "*",
    "EndpointDefaults": {
      "ClientCertificateMode": "AllowCertificate",
      "Protocols": "Http1AndHttp2",
      "SslProtocols": [ "Tls12" ]
    },
    "Logging": {
      "LogLevel": {
        "Default": "Trace",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  }
}